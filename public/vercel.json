{
  "version": 2,
  "rewrites": [
    { "source": "/qris/(.*)", "destination": "/api/server/qris/$1" },
    { "source": "/webhook/(.*)", "destination": "/api/server/webhook/$1" },
    { "source": "/diag", "destination": "/api/server/diag" },
    { "source": "/docs", "destination": "/public/index.html" },
    { "source": "/", "destination": "/public/index.html" }
  ]
}